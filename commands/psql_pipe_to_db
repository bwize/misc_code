# Install postgres 9.3 client (if you don't have it already)
# http://www.postgresql.org/download/linux/ubuntu/
echo "deb http://apt.postgresql.org/pub/repos/apt/ precise-pgdg main" >> /etc/apt/sources.list.d/pgdg.list
wget --quiet -O - https://www.postgresql.org/media/keys/ACCC4CF8.asc | \
  apt-key add -
apt-get update
apt-get install postgresql-client-9.3
# create table on target db:
DROP TABLE IF EXISTS gini_state_mview;
CREATE TABLE gini_state_mview
(
  shid character varying(254),
  name character varying(50),
  household__total__gini__percent double precision
)
WITH (
  OIDS=FALSE
);
ALTER TABLE gini_state_mview
  OWNER TO postgres;
# do the copy
psql -h  10.0.4.28 -U postgres propdata -c "\copy (SELECT shid, name, household__total__gini__percent FROM gini_state_view ORDER BY shid) TO STDOUT" | psql -h 146.148.91.39 -U postgres propdata -c "\copy gini_state_mview (shid, name, household__total__gini__percent) FROM STDIN"





psql -h  10.0.4.28 -U postgres voter_reg -c "\copy (SELECT shid, name, pop__total__registered_voters__count FROM reg_voter_place ORDER BY shid) TO STDOUT" | psql -h 146.148.91.39 -U postgres propdata -c "\copy gini_state_mview (shid, name, pop__total__registered_voters__count) FROM STDIN"



pg_dump -t lai_blockgroup -h  10.0.4.28 -U postgres -d propdata | psql -h 146.148.91.39 -U postgres propdata


pg_dump -t dash_congressionaldistrict_geo -h 146.148.91.39 -U postgres propdata | psql -h  10.0.4.28 -U postgres -d propdata
